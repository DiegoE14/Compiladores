        case 10: break;
        case 3: 
          { ts.add(new Token("Abrir: attributos",yytext()));
          }
        case 11: break;
        case 4: 
          { ts.add(new Token("Cerrar: attributos",yytext()));
          }
        case 12: break;
        case 5: 
          { ts.add(new Token("Atributo condicion",yytext()));
          }
        case 13: break;
        case 6: 
          { ts.add(new Token("Atributo asignacion",yytext()));
          }
        case 14: break;
        case 7: 
          { ts.add(new Token("while: attributos",yytext()));
          }
        case 15: break;
        case 8: 
          { ts.add(new Token("Comando PHP",yytext()));
          }
        case 16: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            return null;
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
